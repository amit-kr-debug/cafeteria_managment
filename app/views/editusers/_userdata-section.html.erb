<div class="container">
      <div class="row">
          <div class="well col-xs-10 col-sm-10 col-md-6 col-xs-offset-1 col-sm-offset-1 col-md-offset-3">
<table class="table table-striped menus-table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Remove</th>
    </tr>
  </thead>
  <tbody>
  <% users.each do |user| %>
    <tr>
      <td><%= user.name%></td>
      <td><%= user.email %></td>
      <td><%= form_with(url: "/editusers/#{user.id}", method: "delete") do %>
               <%= image_submit_tag("/delete.png") %>
            <% end %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</div>
</div>
</div>
